{
	"name": "CURL Command Converter for OSC",
	"version": "0.0.0.2",
	"description": "Extract curl command from OSC log",
	"manifest_version": 2,
	"permissions": ["activeTab", "clipboardRead"],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "app_icon_128x128",
			"32": "app_icon_128x128",
			"48": "app_icon_128x128",
			"128": "app_icon_128x128"
		}
	}
}
