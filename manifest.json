{
	"name": "OSC Curl Command Extractor",
	"version": "1.0",
	"description": "Extract curl command",
	"manifest_version": 2,
	"permissions": ["activeTab", "clipboardRead", "clipboardWrite"],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon.png",
			"32": "icon.png",
			"48": "icon.png",
			"128": "icon.png"
		}
	}
}
