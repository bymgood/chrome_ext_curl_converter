{
	"name": "CURL Command Converter for OSC",
	"version": "0.0.0.3",
	"description": "Extract curl command from OSC log",
	"manifest_version": 2,
	"permissions": ["activeTab", "clipboardRead"],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/icon.png",
			"32": "images/icon.png",
			"48": "images/icon.png",
			"128": "images/icon.png"
		}
	}
}
